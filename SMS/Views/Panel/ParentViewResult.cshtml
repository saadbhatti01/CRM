
@{
    ViewBag.Title = "ParentViewResult";
}

<div class="page-inner">
    @if (TempData["Error"] != null)
    {
        <div class="alert alert-danger alert-dismissible" role="alert" style="margin-bottom:0;">
            <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            Oh snap! @TempData["Error"].
        </div>
    }
    @if (TempData["Success"] != null)
    {
        <div class="alert alert-success alert-dismissible" role="alert" style="margin-bottom:0;">
            <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            Success! @TempData["Success"].
        </div>
    }
    @if (TempData["Info"] != null)
    {
        <div class="alert alert-info alert-dismissible" role="alert" style="margin-bottom:0;">
            <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            Info! @TempData["Info"].
        </div>
    }
    @if (TempData["StdId"] != null)
    {
        foreach (var s in (List<SMS.Models.StdFeeDetail>)TempData["StdId"])
        {
            <div id="main-wrapper">
                <div class="page-title">
                    <h3 class="breadcrumb-header">@s.Stdname's Result</h3>

                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="panel panel-white">
                            <div class="panel-body">
                                <div id="rootwizard">
                                    <div class="row m-b-lg">
                                        <div class="col-md-6">
                                            <form action="~/Panel/ParentViewResult" method="post">
                                                <div class="row">
                                                    <input type="hidden" id="stdId" name="StdId" value="@s.stdId" />
                                                    <div class="form-group col-md-6">
                                                        <label for="ageRangeField">Session</label>
                                                        @Html.DropDownList("sesId", (SelectList)ViewData["Session"], "Select Session", new { required = "required", @class = "form-control" })
                                                    </div>
                                                    <div class="form-group col-md-6">
                                                        <label for="ageRangeField">Exam Type</label>
                                                        @Html.DropDownList("etId", (SelectList)ViewData["ExamType"], "--Select Exam Type--", new { required = "required", @class = "form-control" })
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="form-group col-md-12">
                                                        <input class="btn btn-primary" type="submit" name="search" id="show" value="Show">
                                                    </div>
                                                </div>
                                            </form>
                                            <hr />
                                        </div>
                                        
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>



                </div><!-- Row -->
            </div>
        }
    }

    <!-- Main Wrapper -->

</div>

