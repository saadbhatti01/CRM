@if (TempData["Class"] != null)
{
    <div class="table-responsive">
        <table class="table table-bordered">
            <tr>
                <th>
                    Session
                </th>
                <th>
                    Section
                </th>
                <th>
                    Class
                </th>
                <th>
                    Fee Type
                </th>
                <th>
                    Amount
                </th>
                <th>
                    Max Discount
                </th>
                <th>
                    Action
                </th>
            </tr>

            @foreach (var i in (List<SMS.Models.ClassFeePkg>)TempData["Class"])
            {
                <tr>
                    <td>@i.ses.sesName</td>
                    <td>@i.sec.secName</td>
                    <td>@i.cls.classname</td>
                    <td>@i.ft.feeTypeName</td>
                    <td>@i.cfpAmt</td>
                    <td>@i.cfpDis %</td>
                    <td>

                        <a title="Update Package" class="counter" href="@Url.Action("EditClassFeePkg", new {  id = i.cfpId  })">
                            <i class="fa fa-edit" style="font-size:20px"></i>
                        </a>
                        &nbsp;
                        <a title="Delete Package" class="counter" href="@Url.Action("DelClassFee", new { id = i.cfpId })">
                            <i class="fa fa-trash" style="font-size:20px;color:red"></i>
                        </a>
                    </td>
                </tr>
            }
        </table>

    </div>
}